      "max_analyzed_offset": 1000000
    },
    "max_terms_count": 65536,
    "max_regex_length": 1000,
    "routing": {
      "allocation": {
        "enable": "all"
      },
      "rebalance": {
        "enable": "all"
      }
    },
    "gc_deletes": "60s"
  },
